<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="registry_groups_category" model="ir.module.category">
        <field name="name">Real Estate</field>
        <field name="description">Holds all the different Real Estate Groups</field>
    </record>
    <record id="group_registry_user" model="res.groups">
        <field name="name">Real Estate/User</field>
        <field name="category_id" ref="registry_groups_category" />
    </record>
    <record id="group_registry_admin" model="res.groups">
        <field name="name">Real Estate/Admin</field>
        <field name="implied_ids" eval="[Command.link(ref('group_registry_user'))]" />
        <field name="category_id" ref="registry_groups_category" />
    </record>
</odoo>