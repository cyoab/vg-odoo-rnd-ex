<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="skydiver_list_action" model="ir.actions.act_window">
        <field name="name">Skydivers</field>
        <field name="res_model">skydiver</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create your first skydiver
            </p>
        </field>
    </record>

    <menuitem name="Skydivers"
        id="menu_skydivers"
        sequence="1"
        web_icon="skydivers,static/description/icon.png">
        <menuitem id="menu_skydivers_sub" name="Skydiver Management" sequence="1">
            <menuitem id="menu_skydiver_registry" name="Skydiver Registry" sequence="2" action="skydiver_list_action"/>
        </menuitem>
    </menuitem>
</odoo>